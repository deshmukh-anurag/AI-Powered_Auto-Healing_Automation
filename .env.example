# Environment Variables Template
# Copy this file to .env and fill in your values

# Database Configuration
DATABASE_URL=postgresql://postgres:password@localhost:5432/autoheal

# AI Model Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Vector Database
CHROMA_URL=http://localhost:8000

# Playwright Settings
HEADLESS=true

# Model Selection (gemini-flash | gemini-pro | gpt-4o)
DEFAULT_MODEL=gemini-flash

# Optional: OpenAI API Key (for multi-model support)
# OPENAI_API_KEY=your_openai_key_here

# Optional: Logging Level (debug | info | warn | error)
LOG_LEVEL=info

# Optional: Max concurrent tests
MAX_CONCURRENT_TESTS=3

# Optional: Auto-healing threshold (0.0 to 1.0)
AUTO_HEAL_THRESHOLD=0.8
